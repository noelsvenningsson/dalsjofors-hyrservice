# App runtime port
PORT=8000

# Swish recipient number used in payment payload generation
# Use test/non-production value locally.
SWISH_PAYEE=1234945580

# Optional booking notification webhook URL
NOTIFY_WEBHOOK_URL=

# Optional HMAC secret for webhook signature (X-Notify-Signature)
NOTIFY_WEBHOOK_SECRET=

# Admin auth token for /admin and /api/admin/* routes.
# Required in production. Optional in local development.
ADMIN_TOKEN=

# HMAC secret used to sign admin session cookies.
# Required if you want browser login at /admin/login.
ADMIN_SESSION_SECRET=
